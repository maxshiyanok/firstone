<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>Status</servlet-name>
        <servlet-class>Status</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Status</servlet-name>
        <url-pattern>/status</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetName</servlet-name>
        <servlet-class>GetName</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetName</servlet-name>
        <url-pattern>/get</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetNewPage</servlet-name>
        <servlet-class>GetNewPage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetNewPage</servlet-name>
        <url-pattern>/page</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SendCheckData</servlet-name>
        <servlet-class>SendCheckData</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SendCheckData</servlet-name>
        <url-pattern>/check</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>PageRequestRedirection</servlet-name>
        <servlet-class>PageRequestRedirection</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PageRequestRedirection</servlet-name>
        <url-pattern>/test2</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>StatusRequestForwarder</servlet-name>
        <servlet-class>StatusRequestForwarder</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StatusRequestForwarder</servlet-name>
        <url-pattern>/test1</url-pattern>
    </servlet-mapping>
</web-app>
